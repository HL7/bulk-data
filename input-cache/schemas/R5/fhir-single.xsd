            </xs:annotation>
           <xs:element name="authorReference" type="Reference"  maxOccurs="1"/>
           <xs:element name="authorString" type="string"  maxOccurs="1"/>
          </xs:choice>
          <xs:element name="time" type="dateTime" minOccurs="0" maxOccurs="1">
