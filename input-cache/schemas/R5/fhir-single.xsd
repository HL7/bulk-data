              <xs:documentation xml:lang="en">Estimated total number of participants to be enrolled.</xs:documentation>
           </xs:annotation>
          </xs:element>
          <xs:element name="actualNumber" minOccurs="0" maxOccurs="1" type="unsignedInt">
